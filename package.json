{
  "name": "buildmotion",
  "version": "12.0.1",
  "license": "MIT",
  "scripts": {
    "build": "nx build",
    "clean": "yarn run clean:folders && yarn run clean:packages",
    "clean:folders": "rm -rf dist && rm -rf tmp && rm -rf coverage",
    "clean:packages": "rm -rf node_modules && npm install",
    "lint": "nx run-many --target=lint --all --parallel=true --skip-nx-cache=true --fix",
    "postinstall": "ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "test:coverage": "nx run-many --all --target=test --coverage --parallel=true --skip-nx-cache=true",
    "test": "nx run-many --all --target=test --parallel=true --skip-nx-cache=true",
    "build:pre": "chmod ug+x scripts/build-libs && chmod ug+x scripts/publish-packages",
    "build:libs": "yarn run build:pre && scripts/build-libs",
    "publish:libs": "chmod ug+x scripts/publish-packages && scripts/publish-packages",
    "version:packages": "chmod ug+x scripts/version-packages && scripts/version-packages"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^12.2.0",
    "@angular/common": "^12.2.0",
    "@angular/compiler": "^12.2.0",
    "@angular/core": "^12.2.0",
    "@angular/forms": "^12.2.0",
    "@angular/platform-browser": "^12.2.0",
    "@angular/platform-browser-dynamic": "^12.2.0",
    "@angular/router": "^12.2.0",
    "@datadog/browser-logs": "^4.3.0",
    "@datadog/browser-rum": "^4.3.0",
    "@nrwl/angular": "12.10.1",
    "guid-typescript": "^1.0.9",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular-eslint/eslint-plugin": "~12.3.0",
    "@angular-eslint/eslint-plugin-template": "~12.3.0",
    "@angular-eslint/template-parser": "~12.3.0",
    "@angular/compiler-cli": "^12.2.0",
    "@angular/language-service": "^12.2.0",
    "@nrwl/cli": "12.10.1",
    "@nrwl/eslint-plugin-nx": "12.10.1",
    "@nrwl/jest": "12.10.1",
    "@nrwl/linter": "12.10.1",
    "@nrwl/tao": "12.10.1",
    "@nrwl/workspace": "12.10.1",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.31.1",
    "@typescript-eslint/parser": "~4.31.1",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.2.3",
    "jest-preset-angular": "10.0.1",
    "ng-packagr": "^12.1.1",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.3.5"
  }
}
