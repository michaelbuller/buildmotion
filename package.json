{
  "name": "buildmotion",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "nx build",
    "clean": "yarn run folders:clean && yarn run packages:clean",
    "folders:clean": "rm -rf dist && rm -rf tmp && rm -rf coverage",
    "lint": "nx run-many --target=lint --all --parallel=true --skip-nx-cache=true --fix",
    "packages:clean": "rm -rf node_modules && yarn install",
    "postinstall": "ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "test:coverage": "nx run-many --all --target=test --coverage --parallel=true --skip-nx-cache=true",
    "test": "nx run-many --all --target=test --parallel=true --skip-nx-cache=true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.11",
    "@angular/common": "13.3.11",
    "@angular/compiler": "13.3.11",
    "@angular/core": "13.3.11",
    "@angular/forms": "13.3.11",
    "@angular/platform-browser": "13.3.11",
    "@angular/platform-browser-dynamic": "13.3.11",
    "@angular/router": "13.3.11",
    "@datadog/browser-logs": "^4.3.0",
    "@datadog/browser-rum": "^4.3.0",
    "@nrwl/angular": "13.10.5",
    "guid-typescript": "^1.0.9",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.8",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/compiler-cli": "13.3.11",
    "@angular/language-service": "13.3.11",
    "@nrwl/cli": "13.10.5",
    "@nrwl/eslint-plugin-nx": "13.10.5",
    "@nrwl/jest": "13.10.5",
    "@nrwl/linter": "13.10.5",
    "@nrwl/tao": "12.10.1",
    "@nrwl/workspace": "13.10.5",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.2.3",
    "jest-preset-angular": "11.1.1",
    "ng-packagr": "13.3.1",
    "prettier": "2.7.1",
    "ts-jest": "27.0.5",
    "typescript": "4.6.4"
  }
}

