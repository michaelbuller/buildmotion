{
  "name": "buildmotion",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx build",
    "clean": "yarn run clean:folders && yarn run clean:packages",
    "clean:folders": "rm -rf dist && rm -rf tmp && rm -rf coverage",
    "clean:packages": "rm -rf node_modules && npm install",
    "lint": "nx run-many --target=lint --all --parallel=true --skip-nx-cache=true --fix",
    "postinstall": "ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "test:coverage": "nx run-many --all --target=test --coverage --parallel=true --skip-nx-cache=true",
    "test": "nx run-many --all --target=test --parallel=true --skip-nx-cache=true",
    "build:pre": "chmod ug+x scripts/build-libs && chmod ug+x scripts/publish-packages",
    "build:libs": "yarn run build:pre && scripts/build-libs",
    "publish:libs": "chmod ug+x scripts/publish-packages && scripts/publish-packages",
    "version:packages": "chmod ug+x scripts/version-packages && scripts/version-packages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.0.0",
    "@angular/common": "~14.0.0",
    "@angular/compiler": "~14.0.0",
    "@angular/core": "~14.0.0",
    "@angular/forms": "~14.0.0",
    "@angular/platform-browser": "~14.0.0",
    "@angular/platform-browser-dynamic": "~14.0.0",
    "@angular/router": "~14.0.0",
    "@datadog/browser-logs": "^4.13.0",
    "@datadog/browser-rum": "^4.13.0",
    "@nrwl/angular": "14.3.6",
    "@nrwl/node": "14.3.6",
    "guid-typescript": "^1.0.9",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.0.0",
    "@angular-eslint/eslint-plugin": "~13.2.1",
    "@angular-eslint/eslint-plugin-template": "~13.2.1",
    "@angular-eslint/template-parser": "~13.2.1",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "~14.0.0",
    "@angular/language-service": "~14.0.0",
    "@nrwl/cli": "14.3.6",
    "@nrwl/eslint-plugin-nx": "14.3.6",
    "@nrwl/jest": "14.3.6",
    "@nrwl/js": "14.3.6",
    "@nrwl/linter": "14.3.6",
    "@nrwl/workspace": "14.3.6",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.24.0",
    "@typescript-eslint/parser": "~5.24.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.5.1",
    "jest-preset-angular": "~11.1.2",
    "ng-packagr": "~14.0.0",
    "nx": "14.3.6",
    "postcss": "^8.4.5",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.5.0",
    "postcss-url": "~10.1.3",
    "prettier": "^2.6.2",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2"
  }
}

